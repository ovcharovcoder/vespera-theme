{
  "$schema": "vscode://schemas/color-theme",
  "name": "Vespera Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#0F1115",
    "editor.foreground": "#D8DEE9",
    "editorLineNumber.foreground": "#4D5A68",
    "editorLineNumber.activeForeground": "#8F99A8",
    "editorCursor.foreground": "#2381B2",
    "editor.selectionBackground": "#2381B220",
    "editor.inactiveSelectionBackground": "#2381B210",
    "editor.lineHighlightBackground": "#1A2734",
    "editor.lineHighlightBorder": "#2381B215",
    "editorBracketMatch.border": "#2381B240",
    "editorBracketMatch.background": "#2381B210",

    "editorGutter.modifiedBackground": "#256FB3",
    "editorGutter.addedBackground": "#00A68A",
    "editorGutter.deletedBackground": "#E34A4F",
    "editorWhitespace.foreground": "#26313D",

    "activityBar.background": "#0C0E12",
    "activityBar.foreground": "#AAB6C2",
    "sideBar.background": "#0F1115",
    "sideBar.foreground": "#9AA5B0",

    "statusBar.background": "#0C0E12",
    "statusBar.foreground": "#8F99A8",

    "tab.activeBackground": "#13161B",
    "tab.activeForeground": "#E3E7EC",
    "tab.inactiveBackground": "#0C0E12",
    "tab.inactiveForeground": "#7C8895",

    "terminal.foreground": "#D8DEE9",
    "terminal.background": "#0F1115",

    "titleBar.activeBackground": "#0C0E12",
    "titleBar.activeForeground": "#AAB6C2",

    "focusBorder": "#2381B240",
    "list.activeSelectionBackground": "#003B4F50",
    "list.hoverBackground": "#191F28"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6B7585",
        "fontStyle": ""
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#2381B2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["variable", "meta.definition.variable"],
      "settings": {
        "foreground": "#B48EAD"
      }
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {
        "foreground": "#5CC99A"
      }
    },
    {
      "scope": ["number", "constant.numeric"],
      "settings": {
        "foreground": "#A97BE0"
      }
    },
    {
      "scope": ["function", "entity.name.function"],
      "settings": {
        "foreground": "#6EA5E0"
      }
    },
    {
      "scope": ["entity.name.class", "support.class", "meta.class"],
      "settings": {
        "foreground": "#DDBB65"
      }
    },
    {
      "scope": ["support.function", "support.constant"],
      "settings": {
        "foreground": "#6FB8D6"
      }
    },
    {
      "scope": ["constant.language", "variable.language"],
      "settings": {
        "foreground": "#F48C4A"
      }
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {
        "foreground": "#FFFFFF",
        "background": "#E34A4F"
      }
    },
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {
        "foreground": "#D8DEE9"
      }
    },
    {
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {
        "foreground": "#2381B2",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#D8DEE9"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#D8DEE9"
      }
    },
    {
      "scope": ["meta.tag", "entity.name.tag"],
      "settings": {
        "foreground": "#4A9DC7"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#6FB7AE"
      }
    },
    {
      "scope": ["punctuation.definition.tag", "punctuation.separator.key-value"],
      "settings": {
        "foreground": "#5A8EB0"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#00BFA5"
      }
    }
  ]
}
