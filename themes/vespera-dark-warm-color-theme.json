{
  "$schema": "vscode://schemas/color-theme",
  "name": "Vespera Dark Warm",
  "type": "dark",
  "colors": {
    "editor.background": "#141317",
    "editor.foreground": "#F2E9D8",
    "editorLineNumber.foreground": "#6B6154",
    "editorLineNumber.activeForeground": "#B2A68F",
    "editorCursor.foreground": "#FF9F40",
    "editor.selectionBackground": "#FF9F4022",
    "editor.inactiveSelectionBackground": "#FF9F4010",
    "editor.lineHighlightBackground": "#26211A",
    "editor.lineHighlightBorder": "#FF9F4015",
    "editorBracketMatch.border": "#FF9F4050",
    "editorBracketMatch.background": "#FF9F4010",
    "editorGutter.modifiedBackground": "#FFAA00",
    "editorGutter.addedBackground": "#8BC34A",
    "editorGutter.deletedBackground": "#EF5350",
    "editorWhitespace.foreground": "#3D362B",
    "activityBar.background": "#121014",
    "activityBar.foreground": "#C8BFA9",
    "sideBar.background": "#16141A",
    "sideBar.foreground": "#B9AF9B",
    "statusBar.background": "#121014",
    "statusBar.foreground": "#B2A68F",
    "tab.activeBackground": "#1A171D",
    "tab.activeForeground": "#F2E9D8",
    "tab.inactiveBackground": "#121014",
    "tab.inactiveForeground": "#9D927A",
    "terminal.foreground": "#F2E9D8",
    "terminal.background": "#141317",
    "titleBar.activeBackground": "#121014",
    "titleBar.activeForeground": "#C8BFA9",
    "focusBorder": "#FF9F4050",
    "list.activeSelectionBackground": "#4E342255",
    "list.hoverBackground": "#2B241C"
  },
  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {"foreground": "#8C7E66","fontStyle": ""}
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {"foreground": "#FF8265","fontStyle": "bold"}
    },
    {
      "scope": ["variable", "meta.definition.variable"],
      "settings": {"foreground": "#F07F8A"}
    },
    {
      "scope": ["string", "constant.other.symbol"],
      "settings": {"foreground": "#FFC542"}
    },
    {
      "scope": ["number", "constant.numeric"],
      "settings": {"foreground": "#F07F8A"}
    },
    {
      "scope": ["function", "entity.name.function"],
      "settings": {"foreground": "#FF8265"}
    },
    {
      "scope": ["entity.name.class", "support.class", "meta.class"],
      "settings": {"foreground": "#FFAA00"}
    },
    {
      "scope": ["support.function", "support.constant"],
      "settings": {"foreground": "#FFC542"}
    },
    {
      "scope": ["constant.language", "variable.language"],
      "settings": {"foreground": "#FF6F43"}
    },
    {
      "scope": ["invalid", "invalid.illegal"],
      "settings": {"foreground": "#FFFFFF","background": "#D32F2F"}
    },
    {
      "scope": ["meta.embedded", "source.groovy.embedded"],
      "settings": {"foreground": "#F2E9D8"}
    },
    {
      "scope": ["markup.heading", "entity.name.section"],
      "settings": {"foreground": "#FF8265","fontStyle": "bold"}
    },
    {
      "scope": ["markup.bold"],
      "settings": {"fontStyle": "bold","foreground": "#F2E9D8"}
    },
    {
      "scope": ["markup.italic"],
      "settings": {"fontStyle": "italic","foreground": "#F2E9D8"}
    },
    {
      "scope": ["meta.tag", "entity.name.tag"],
      "settings": {"foreground": "#FF8265"}
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {"foreground": "#8BC34A"}
    },
    {
      "scope": ["punctuation.definition.tag", "punctuation.separator.key-value"],
      "settings": {"foreground": "#A18B7F"}
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {"foreground": "#8BC34A"}
    }
  ]
}

